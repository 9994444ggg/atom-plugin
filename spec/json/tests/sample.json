{
  "description": "opening a whitelisted file should send a focus event",
  "setup": {
    "default_whitelist": true,
    "kited": "authenticated"
  },
  "actions": [
    {
      "type": "focus",
      "properties": {
        "file": "data/whitelisted/file.py",
        "offset": 11
      }
    }
  ],
  "expect": [
    {
      "type": "request",
      "properties": {
        "path": "/clientapi/editor/event",
        "method": "POST",
        "body": "data/http/event/file_request_focus.json"
      }
    }
  ]
}
